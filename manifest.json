{
  "manifest_version": 2,

  "name": "Dark Atlassian",
  "version": "0.0.1",
  "description": "Friendly dark mode extension for Atlassian Cloud products",

  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["dark_atlassian_scripts/jquery-3.5.1.min.js", "script.js"],
    "matches": ["https://*.atlassian.net/*"]
  }],

  "background": {
    "scripts": ["dark_atlassian_scripts/jquery-3.5.1.min.js"]
  }
}
